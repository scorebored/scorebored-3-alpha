  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "jest && yarn lint",
    "package": "electron-forge package && (cd out ; tar cf scorebored-linux-x64.tar.gz scorebored-linux-x64)",
    "package-pi": "electron-forge --arch=armv7l package && (cd out ; tar cf scorebored-linux-armv7l.tar.gz scorebored-linux-armv7l)",
    "make": "electron-forge make",
    "lint": "eslint --cache --color --ext .js src",
    "clean": "rm -rf out"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017-node7": "^0.5.2",
    "babel-preset-react": "^6.24.1",
    "electron-devtools-installer": "^2.2.1",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.9",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "deep-equal": "^1.0.1",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
