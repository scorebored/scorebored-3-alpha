{
  "application/javascript": {
    "sourceMaps": "inline",
    "presets": [
      "es2017-node7",
      "react"
    ],
    "plugins": [
      ["babel-plugin-root-import", {
        "rootPathSuffix": "src",
        "rootPathPrefix": "@"
      }],
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  }
}