{
  "application/javascript": {
    "sourceMaps": "inline",
    "presets": [
      "es2017-node7",
      "react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "@": "./src"
          }
        }
      ],
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  }
}