{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "semi": [2, "never"],
      "quotes": [2, "single"],
      "no-console": [2, {
        "allow": ["warn", "error"]
      }]
    }
  }
